@model List<User>
@{
    foreach (User user in Model)
    {
        <div>
            <h2>First Name: @user.FirstName</h2>
            <h2>Last Name: @user.LastName</h2>
            <h2>Email: @user.Email</h2>
            <br/>
        </div>
    }
}
